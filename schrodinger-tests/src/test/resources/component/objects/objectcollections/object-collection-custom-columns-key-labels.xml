<objectCollection oid="kker398e-ff47-4b11-b92b-74ff21dbb299">
    <name>Custom columns labels test</name>
    <type>UserType</type>
    <filter>
        <all/>
    </filter>
    <defaultView>
        <column>
            <name>customNameColumn</name>
            <path>name</path>
            <display>
                <label>
                    <orig>Name (custom)</orig>
                    <translation>
                        <key>pageAdminFocus.button.enable</key>
                    </translation>
                </label>
            </display>
        </column>
        <column>
            <name>customRoleMembershipColumn</name>
            <path>roleMembershipRef</path>
            <display>
                <label>
                    <orig>Role membership</orig>
                    <translation>
                        <key>pageAdminFocus.button.disable</key>
                    </translation>
                </label>
            </display>
            <previousColumn>customNameColumn</previousColumn>
        </column>
        <column>
            <name>customPreferredLanguageColumn</name>
            <path>preferredLanguage</path>
            <display>
                <label>
                    <orig>Preferred language</orig>
                    <translation>
                        <key>pageAdminFocus.button.unlink</key>
                    </translation>
                </label>
            </display>
            <previousColumn>customRoleMembershipColumn</previousColumn>
        </column>
    </defaultView>
</objectCollection>
