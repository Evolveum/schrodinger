<objectCollection oid="kk3pk98e-f587-4b11-b92b-723251dbb299">
    <name>Custom columns view</name>
    <type>UserType</type>
    <filter>
        <all/>
    </filter>
    <defaultView>
        <column>
            <name>customNameColumn</name>
            <path>name</path>
            <display>
                <label>Name (custom)</label>
            </display>
        </column>
        <column>
            <name>customRoleMembershipColumn</name>
            <path>roleMembershipRef</path>
            <display>
                <label>Role membership</label>
            </display>
            <previousColumn>customNameColumn</previousColumn>
        </column>
        <column>
            <name>customPreferredLanguageColumn</name>
            <path>preferredLanguage</path>
            <display>
                <label>Preferred language</label>
            </display>
            <previousColumn>customRoleMembershipColumn</previousColumn>
        </column>
    </defaultView>
</objectCollection>
